    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/coverage-v8": "^0.34.3",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.1.0",
    "eslint": "^8.48.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.1.6",
